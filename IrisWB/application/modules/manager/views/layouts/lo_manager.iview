<!doctype html>
<html>
    <head>
        <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
        <link rel="stylesheet" type="text/css" href="/css/wb.css">
        {siteIcon()}
        {autoResource()}
    </head>
    <body {dojo_body()}{bodyOnload()}{wbColors('ORANGE3')}>
        <div id="main">
            {ILO_adminToolBar()}
            <div id="container">
                <div id='Title'>
                    <h1>Iris Workbench Manager</h1>
                </div><!-- Title -->
                <div class="column one">
                    {islet('islSides',[],'left')}
                </div><!-- 1 -->
                <div class="column two">
                    {mainView()}
                </div><!-- 2 -->
                <div class="column three ">
                    {islet('islSides',[],'right')}
                </div><!-- 3 -->
            </div><!-- container -->
            <div id="foot">
                {wbFooter('modules/manager/views/scripts/lo_manager.iview')}
            </div><!--foot-->
        </div><!--main--> 
    </body>
</html>