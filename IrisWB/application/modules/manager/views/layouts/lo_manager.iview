<!doctype html>
<html>
    <head>
        {head()}
        {wbCSS()}
    </head>
    <body {dojo_body()}{wbColors('YELLOW1')}>
        <div id="main">
            {ILO_adminToolBar()}
            <div id="container">
                <div id='Title'>
                    <h1>Iris Workbench Manager</h1>
                </div><!-- Title -->
                <div class="column one">
                    {islet('islSides',['sectionMode' => $sectionMode],'left')}
                </div><!-- 1 -->
                <div class="column two">
                    {mainView()}
                </div><!-- 2 -->
                <div class="column three ">
                    {islet('islSides',[],'right')}
                </div><!-- 3 -->
            </div><!-- container -->
            <div id="foot">
                {wbFooter()}
            </div><!--foot-->
        </div><!--main--> 
    </body>
</html>