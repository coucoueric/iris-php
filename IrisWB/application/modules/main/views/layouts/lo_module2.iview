<!doctype html>
<html>
    <!-- This layout is used only as an exemple of a module defined layout -->
    <head>
        {head()}
        {styleLoader("wb2.css")}
    </head>
    <body {dojo_body()}{bodyOnload()}{wbColors('ORANGE3')}>
        <div id="main">
            {ILO_adminToolBar()}
            <div id="container">
                <div id='Title'>
                    <h1>{Title}</h1>
                </div><!-- Title -->
                <div class="column one">
                    <h2>Column 1</h2>
                    {loremIpsum($left)}
                </div><!-- 1 -->
                <div class="column two">
                    {mainView()}
                </div><!-- 2 -->
                <div class="column three ">
                    <h2>Column 3</h2>
                    {loremIpsum($right)}
                </div><!-- 3 -->
            </div><!-- container -->
            <div id="foot">
                {wbFooter('modules/main/views/scripts/lo_module2.iview')}
            </div><!--foot-->
        </div><!--main-->  
    </body>
</html>